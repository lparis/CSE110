package customerPackage;

public class TestCustomer {
	
	public static void main(String[] args) {
		TestDefaultCustomer();
		TestPrivilegedCustomer();
		TestRestrictedCustomer();
	}

	public static void TestDefaultCustomer() {
		Customer normCust = new Customer(1234, "Johnson", null);
	}

	public static void TestPrivilegedCustomer() {
		Customer privCust = new Customer(1234, "Swanson", "Privileged");
	}
	
	public static void TestRestrictedCustomer() {
		Customer restCust = new Customer(1234, "Jefferson", "Restricted");
	}

}
